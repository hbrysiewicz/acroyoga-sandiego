<div class="w-section section">
  <div class="w-container">
    <div class="w-dyn-list">
      <div class="w-dyn-items w-row w-clearfix">
        {{#each model as |event|}}
          <div class="w-dyn-item w-col w-col-4 project-block">
            {{#link-to 'events.show' event classNames='w-inline-block project-link'}}
              <div class="project-image" style='background-image: url({{event.image}}'></div>
              <div class="project-tag">{{event.status}}</div>
              <div class="project-title">{{event.name}}</div>
            {{/link-to}}
          </div>
        {{else}}
          <div class="w-dyn-empty">
            <p>Oops! There's nothing here yet... Check back later!</p>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
